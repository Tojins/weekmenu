# Production Supabase (current remote)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase project reference (for CLI commands)
SUPABASE_PROJECT_REF=your_project_id

# Database password (needed for db:push and migrations)
# Get this from your Supabase dashboard > Settings > Database
SUPABASE_DB_PASSWORD=your_database_password

# Service role key (for admin access - bypasses RLS)
# Get this from Supabase dashboard > Settings > API > service_role key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
